        "# Copy function\n",
        "\n",
        "Copy a given dictionary dict1 to a new dictionary dict2."
      ],
      "metadata": {
        "id": "CPMQnwTRLEBZ"
      }
    },
    {
      "cell_type": "code",
      "execution_count": 1,
      "metadata": {
        "id": "SmSWcDGOK9_K"
      },
      "outputs": [],
      "source": [
        "def copyDictionary(dict1):\n",
        "  dict2=dict1.copy()\n",
        "  print(dict2)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "dict1 = {\"a\":1,\"b\":2,\"c\":3,\"d\":4}\n",
        "copyDictionary(dict1)"
